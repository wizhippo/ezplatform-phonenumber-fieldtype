{
    "name": "wizhippo/ezplatform-phonenumber-fieldtype",
    "description": "Repository dedicated to PhoneNumber Field Type for eZ Platform",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0-only",
    "keywords": [
        "ezplatform",
        "ezpublish",
        "wizhippo",
        "phonenumber"
    ],
    "authors": [
        {
            "name": "Douglas Hammond",
            "homepage": "https://github.com/wizhippo/ezplatform-phonenumber-fieldtype/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wizhippo\\WizhippoPhoneNumberFieldtypeBundle\\": "src/bundle/",
            "Wizhippo\\WizhippoPhoneNumberFieldtype\\": "src/lib/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ezsystems/ezpublish-kernel": "^8.0",
        "symfony/http-kernel": "^4.3",
        "symfony/console": "^4.3",
        "symfony/config": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/form": "^4.3",
        "symfony/options-resolver": "^4.3",
        "giggsey/libphonenumber-for-php": "^8.20"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
